<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net7.0</TargetFrameworks>
	    <PackageProjectUrl>https://www.nuget.org/packages/EasilyNET.Mongo.ConsoleDebug</PackageProjectUrl>
        <Description>MongoDB.ConsoleDebug,用来实现类似EF将命令语句输出到控制台的功能.</Description>
	    <LangVersion>preview</LangVersion>
	    <RepositoryUrl>https://github.com/EasilyNET/EasilyNET</RepositoryUrl>
	    <Nullable>enable</Nullable>
	    <Authors>joesdu</Authors>
	    <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

	<ItemGroup>
        <PackageReference Include="MongoDB.Driver.Core" Version="2.19.0" />
    </ItemGroup>

</Project>
